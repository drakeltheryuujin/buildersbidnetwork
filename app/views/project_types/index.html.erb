<h1>Listing project_types</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Edit</th>
    <th>Destroy</th>
  </tr>

<% @project_types.each do |project_type| %>
  <tr>
    <td><%= link_to project_type.name, project_type %></td>
    <td><%= link_to 'Edit', edit_project_type_path(project_type), :class => 'nav nav-edit' %></td>
    <td><%= link_to 'Destroy', project_type, {:confirm => 'Are you sure?', :method => :delete, :class => 'nav nav-destroy' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Project type', new_project_type_path %>
