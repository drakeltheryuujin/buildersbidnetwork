<div class="row login-page create-profile">
  <div class="offset2 span12 form-stacked">
    <div class="login-box">
      <div class="login-head">
        <h2>Let's get started by creating your company profile.</h2>
      </div>

      <%= nested_form_for @profile do |f| %>
        <%= render :partial => 'form', :locals => { :f => f } %>
      <% end %>
    </div>
  </div>
</div>
